<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="scmt_manual" >
    <select id="getListManual" parameterType="map" resultType="com.dibimbing.dibimbing.mybatis.model.BarangMybatis">
        select resid,  resnama,  resstok,  resharga,  ressatuan from public.getbarang1(
        #{rqnama,mode=IN,jdbcType=VARCHAR})
    </select>

    <select id="getById" parameterType="map" resultType="com.dibimbing.dibimbing.mybatis.model.BarangMybatis">
        select resid,  resnama,  resstok,  resharga,  ressatuan from public.getbyidfunction(
        #{rqid,mode=IN,jdbcType=NUMERIC})
    </select>

    <insert id="testsptyu" parameterType="map" statementType="CALLABLE">
        {CALL public.testsp(#{rqid,mode=IN,jdbcType=VARCHAR},
        #{resid,mode=OUT,jdbcType=VARCHAR})}
    </insert>
</mapper>
